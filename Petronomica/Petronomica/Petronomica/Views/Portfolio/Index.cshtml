<div id="begin"></div>

@{await Html.RenderPartialAsync("_PortfolioInfo");}

<div class="container">
    <div class="jumbotron">

        <div class="rating">
            <i class="staricon bg-danger"></i>
            <i class="staricon">   </i>
            <i class="staricon"></i>
            <i class="staricon"></i>
